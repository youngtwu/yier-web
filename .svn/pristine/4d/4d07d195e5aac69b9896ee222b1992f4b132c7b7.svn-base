  <!-- 根据条件（患者信息）查询医生信息（患者关注信息attention） -->
  <select id="getPharmacistPoById" parameterType="com.yier.platform.common.requestParam.PharmacistRequest"  resultMap="getPharmacistPoByIdMap">
    select IF(atotal.attention IS NULL,0,atotal.attention) attentionTotal, if(tc.chat IS NULL, t.chat , tc.chat) chat,if(aa.attention IS NULL, 0 , aa.attention) attention, dt.`name` doctorTitle ,t.imtoken,t.doctor_card_no doctorCardNo, t.hospital_id hospitalId, t.catalog_id catalogId ,t.birthday,
    t.sex, t.check_url checkUrl, t.photo_url photoUrl, t.id, t.true_name trueName, t.avatar_url avatarUrl, t.id_card_no idCardNo, t.phone_number phoneNumber, t.pass_hash passHash, t.pass_salt passSalt, t.department_id departmentId,
    t.title_id titleId, t.field, t.visit, t.practice_point practicePoint, t.status, t.gmt_create gmtCreate, t.gmt_modified gmtModified,t.profile, t.remarks
    from t_pharmacist t LEFT JOIN t_pharmacist_title dt ON t.title_id = dt.id  LEFT JOIN t_attention_user aa ON t.id = aa.attention_user_id and aa.attention_type_id = 3 and aa.type_id = 1
    <if test="patientId != null">
      and aa.user_id = #{patientId}
    </if>
    LEFT JOIN t_chat_state tc ON t.id = replace(tc.chat_space,concat('patient',#{patientId},'pharmacist'), '')
    LEFT JOIN (
    SELECT sum(atotalGroup.attention) attention,atotalGroup.attention_type_id ,atotalGroup.attention_user_id from t_attention_user atotalGroup
    <where>
      atotalGroup.attention_type_id = 3
      <if test="pharmacistId != null">
        and atotalGroup.attention_user_id = #{pharmacistId}
      </if>
    </where>
    GROUP BY atotalGroup.attention_type_id ,atotalGroup.attention_user_id
    ) atotal
    ON atotal.attention_type_id = 3 AND atotal.attention_user_id = t.id
    <where>
      <if test="pharmacistId != null">
        and t.id = #{pharmacistId}
      </if>
    </where>
    limit 1
  </select>




