  <select id="listMedicalOrderPo" parameterType="com.yier.platform.common.requestParam.MedicalOrderRequest" resultMap="MedicalOrderPoMap">
    select <include refid="Base_Po_Column_List" /> from t_medical_order t
    <where>
      <if test="medicalOrderId != null">
        and t.id = #{medicalOrderId}
      </if>
      <if test="orderNo != null">
        and t.order_no = #{orderNo}
      </if>
      <if test="hospitalId != null">
        and t.hospital_id = #{hospitalId}
      </if>
      <if test="patientId != null">
        and t.patient_id = #{patientId}
      </if>
      <if test="doctorId != null">
        and t.doctor_id = #{doctorId}
      </if>
      <choose>
        <when test="@org.apache.commons.lang3.StringUtils@isNotBlank( statusDetail )">
          and t.status_detail = #{statusDetail}
        </when>
        <when test="statusDetail != null">
          and t.status_detail != #{statusDetail}
        </when>
        <otherwise>

        </otherwise>
      </choose>
      <choose>
        <when test="@org.apache.commons.lang3.StringUtils@isNotBlank( status )">
          and t.status = #{status}
        </when>
        <when test="status != null">
          and t.status != #{status}
        </when>
        <otherwise>
        </otherwise>
      </choose>
    </where>
    <choose>
      <when test="sort!=null and order!=null">
        order by ${sort} ${order}
      </when>
      <otherwise>
        order by t.id desc
      </otherwise>
    </choose>
    <choose>
      <when test="start!=null and size!=null">
        limit #{start},#{size}
      </when>
      <when test="start==null and size!=null">
        limit #{size}
      </when>
    </choose>
  </select>

