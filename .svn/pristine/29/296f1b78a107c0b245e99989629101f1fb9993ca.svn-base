package com.yier.platform.common.model;

import com.google.common.collect.Lists;

import java.util.List;

public class MedicalPrescriptionPo extends MedicalPrescription {
    private MedicalOrder medicalOrder ;
    private List<OrderDrugs> drugList = Lists.newArrayList();

    public MedicalOrder getMedicalOrder() {
        return medicalOrder;
    }

    public void setMedicalOrder(MedicalOrder medicalOrder) {
        this.medicalOrder = medicalOrder;
    }

    public List<OrderDrugs> getDrugList() {
        return drugList;
    }

    public void setDrugList(List<OrderDrugs> drugList) {
        this.drugList = drugList;
    }
}
