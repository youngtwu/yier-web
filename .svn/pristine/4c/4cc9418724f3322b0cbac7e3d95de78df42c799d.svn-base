package com.yier.platform.common.model;

public class UserRolePo extends UserRole {
    private User user;
    private Role role;

    public UserRolePo() {
    }

    public User getUser() {
        return user;
    }

    public void setUser(User user) {
        this.user = user;
    }

    public Role getRole() {
        return role;
    }

    public void setRole(Role role) {
        this.role = role;
    }
}
